<script language="Javascript">
	var TempHost=location.host;
	function mainSearchHandler(){
		var entry=document.mainSearchForm.mainQuery.value;

		var regex = new RegExp("[\\?&]F=([^&#]*)");
  		var qs = regex.exec(window.location.href);
  		var fValue="";

		if ((entry != '') && (entry != 'Search')){
			cmCreatePageElementTag('Search','SearchResultsPage');
			if ((document.domain).indexOf('safc') >= 0)
				window.location.href=protocolString + "://" + TempHost + "/catalog/search?interface=All&term="+encodeURI(entry)+"&lang=en&region=US&focus=product&N=0+220003048";
			else
				window.location.href=protocolString + "://" + TempHost + "/catalog/search?interface=All&term="+encodeURI(entry)+"&N=0&focus=product&lang=en&region=US";

			//window.location.href=protocolString + "://" + TempHost + "/catalog/search/SearchResultsPage?Query=#TEXT#&Scope=SearchAll&F=".replace(/#TEXT#/g,encodeURI(entry))+fValue + n25ParamStr;
		}else {
				alert ("Please enter a search string.");
				document.mainSearchForm.mainQuery.focus();
		}
		}
	function setMainSearchValue(){
		document.mainSearchForm.mainQuery.focus();
		document.mainSearchForm.value = "";
		}

</script>
			<div id="searchWithin" class="clearfix noPrint">
                    <div class="searchBar">
                      <form id="mainSearchForm" name="mainSearchForm" action="javascript:mainSearchHandler()" method="POST" accept-charset="utf-8">
                        <div class="searchWithinInputContainer">
                        <input type="text" name="mainQuery" id="mainQuery" value="" onfocus="this.value = this.value;" class="searchWithinInput">
                        </div>




                        <input type="hidden" name="N25" id="N25" value="" />
                        <input type="submit" value="" id="submitSearch" class="searchWithinButton results-sprite">
                      </form>
